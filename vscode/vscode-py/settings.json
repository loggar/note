{
  "python.pythonPath": "${workspaceRoot}/.venv/Scripts/python.exe",
  "python.linting.pylintArgs": [
    "--disable=C0103",
    "--disable=C0111",
    "--disable=C0301",
    "--disable=C0321",
    "--disable=E0401",
    "--disable=W0312",
    "--disable=W0231",
    "--disable=W0703"
  ],
  "editor.detectIndentation": false,
  "python.jediEnabled": false,
  "python.linting.pylintEnabled": true
}
