{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program .ts out",
      "program": "${fileDirname}/${fileBasenameNoExtension}.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ts-node .ts",
      "runtimeArgs": ["-r", "ts-node/register"],
      "args": ["${file}"]
    }
  ]
}
